// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("PRISMA_DATABASE_URL")
}

model Download {
  id        Int      @id @default(autoincrement())
  version   String
  date      DateTime @default(now())
  day       String   // YYYY-MM-DD
}

model VirusTotalLink {
  id        Int      @id @default(autoincrement())
  version   String
  url       String
  createdAt DateTime @default(now())
}